<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">

    <changeSet id="3" author="god">
        <createTable tableName="topic">
            <column name="id" type="BIGSERIAL">
                <constraints primaryKey="true"/>
            </column>
            <column name="exam_id" type="BIGINT"/>
            <column name="topic_name" type="VARCHAR(255)"/>
            <column name="correct" type="INT"/>
            <column name="wrong" type="INT"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
